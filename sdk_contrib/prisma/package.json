{
  "name": "aws-xray-sdk-prisma",
  "version": "3.11.1",
  "description": "AWS X-Ray Patcher for prisma (Javascript)",
  "author": "Cosva",
  "contributors": [
    "Eduard Castellanos <eduard@castellanos.dev>"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">= 16.x"
  },
  "directories": {
    "test": "test"
  },
  "peerDependencies": {
    "@prisma/client": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "aws-xray-sdk-core": "^3.5.0"
  },
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "prisma:reset": "prisma db push --schema=./test/prisma/schema.prisma --force-reset",
    "test": "npm run prisma:reset && mocha --recursive ./test/ -R spec && tsd",
    "test-d": "tsd",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "amazon",
    "api",
    "aws",
    "prisma",
    "xray",
    "x-ray",
    "x ray"
  ],
  "license": "Apache-2.0",
  "repository": "https://github.com/aws/aws-xray-sdk-node/tree/master/packages/prisma",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "prisma": "^5.22.0",
    "eslint-plugin-promise": "^6.1.1"
  }
}