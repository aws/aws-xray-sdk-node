generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
  output     = "../../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model Company {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
